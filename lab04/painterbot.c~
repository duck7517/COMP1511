#include <stdio.h>

#define CANVAS_SIZE 36

int main(void) {
    int canvas[CANVAS_SIZE] = {0};
    int input;
    
    
    //scan in numbers until ctrl-d
    
    while (scanf("%d", &input) == 1) {
        canvas[input] = 1;
    }
    
    int i = 0;
    while (i < CANVAS_SIZE) {
        printf("%d ", canvas[i])
    }
    printf("\n");
    
    return 0;
}
